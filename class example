package oopslabb;

class person{
	private String name ;
	private int age ;
	public void setName(String name) {
		this.name=name;
	}
	public String getName() {
		return name;
	}
	public void  setAge(int age) {
		this.age=age;
	}
	public int getAge() {
		return age;
	}
}

public class Char {

	public static void main(String[] args) {
		person p =new person();
		p.setName("Alice");
		p.setAge(30);
		System.out.println(p.getName()+" is "+p.getAge()+"years old");

	}

}
